{
  "name": "fale-ai",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json && tsc-alias",
    "test:migration:run": "npm run test:load-envs prisma migrate deploy",
    "test:migration:generate": "npm run test:load-envs prisma migrate dev",
    "test:load-envs": "dotenv -e .env.test",
    "test:seed": "npm run test:load-envs prisma db seed",
    "dev:load-envs": "dotenv -e .env.development",
    "dev:migration:run": "npm run dev:load-envs prisma migrate deploy",
    "dev:migration:generate": "npm run dev:load-envs prisma migrate dev",
    "dev:seed": "npm run dev:load-envs prisma db seed",
    "dev": "nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register ./src/server.ts' -e ts",
    "test": "npm run test:load-envs jest --runInBand",
    "test:cov": "npm run test:load-envs jest --coverage --runInBand",
    "migrate": "npx prisma migrate dev",
    "generate": "npx prisma generate",
    "start": "ts-node -r tsconfig-paths/register ./dist/server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "precommit": "lint-staged"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@brazilian-utils/brazilian-utils": "^1.0.0-rc.12",
    "@prisma/client": "^5.6.0",
    "@types/axios": "^0.14.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^7.0.1",
    "fs": "^0.0.1-security",
    "http-status": "^1.7.3",
    "https": "^1.0.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "openai": "^4.52.7",
    "qs": "^6.12.1",
    "reflect-metadata": "^0.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.5",
    "@types/node-cron": "^3.0.11",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "prisma": "^5.6.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  }
}
